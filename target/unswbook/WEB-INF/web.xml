<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <welcome-file-list>
    <welcome-file>/WEB-INF/pages/login.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>ControllServlet</servlet-name>
    <servlet-class>com.unsw.Controller.ControllServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ControllServlet</servlet-name>
    <!--1. jsp的 form action = register, 与web.XML文件servlet map-->
    <url-pattern>/control</url-pattern>
    <!--<url-pattern>/*</url-pattern>-->
  </servlet-mapping>

  <servlet>
    <servlet-name>Controlloginservlet</servlet-name>
    <servlet-class>com.unsw.Controller.ControllServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Controlloginservlet</servlet-name>
    <url-pattern>/control/home</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ControlSearchServlet</servlet-name>
    <servlet-class>com.unsw.Controller.ControllServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ControlSearchServlet</servlet-name>
    <url-pattern>/control/result</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ControlAdSearchServlet</servlet-name>
    <servlet-class>com.unsw.Controller.ControllServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ControlAdSearchServlet</servlet-name>
    <url-pattern>/control/adresult</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ControlAgreeFriendServlet</servlet-name>
    <servlet-class>com.unsw.Controller.ControllServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ControlAgreeFriendServlet</servlet-name>
    <url-pattern>/control/agreefriend</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>FileUpLoad</servlet-name>
    <servlet-class>com.unsw.Controller.FileUpLoad</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileUpLoad</servlet-name>
    <url-pattern>/control/FileUpLoad</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>HeadPhotoUpLoad</servlet-name>
    <servlet-class>com.unsw.Controller.HeadPhotoUpLoad</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HeadPhotoUpLoad</servlet-name>
    <url-pattern>/control/HeadPhotoUpLoad</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ControlAdminServlet</servlet-name>
    <servlet-class>com.unsw.Controller.ControllServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ControlAdminServlet</servlet-name>
    <url-pattern>/control/admin</url-pattern>
  </servlet-mapping>

</web-app>
