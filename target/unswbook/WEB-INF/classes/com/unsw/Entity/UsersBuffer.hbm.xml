<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.unsw.Entity.UsersBuffer" table="users_buffer" schema="unswbook">
        <id name="uid">
            <column name="uid" sql-type="int(12)"/>
        </id>
        <property name="adminUid">
            <column name="admin_uid" sql-type="int(12)"/>
        </property>
        <property name="username">
            <column name="username" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="headphotoPath">
            <column name="headphoto_path" sql-type="varchar(128)" length="128"/>
        </property>
        <property name="gender">
            <column name="gender" sql-type="varchar(16)" length="16"/>
        </property>
        <property name="birthday">
            <column name="birthday" sql-type="date"/>
        </property>
		<property name="verification">
            <column name="verification" sql-type="char(6)"/>
        </property>
        <property name="registerDate">
            <column name="register_date" sql-type="datetime"/>
        </property>
        <property name="loginDate">
            <column name="login_date" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>