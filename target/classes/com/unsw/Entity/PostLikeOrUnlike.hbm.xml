<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.unsw.Entity.PostLikeOrUnlike" table="postlikeorunlike" schema="unswbook">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="uid">
            <column name="sender_uid" sql-type="int(12)"/>
        </property>
        <property name="postId">
            <column name="post_id" sql-type="int(11)"/>
        </property>
        <property name="sendTime">
            <column name="send_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>