<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.unsw.Entity.Photo" table="photo" schema="unswbook">
        <id name="photoId">
            <column name="photo_id" sql-type="int(11)"/>
        </id>
        <property name="albumId">
            <column name="album_id" sql-type="int(11)"/>
        </property>
        <property name="postId">
            <column name="post_id" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>